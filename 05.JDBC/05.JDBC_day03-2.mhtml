					+ " where id =3D '" + to + "'";
			// =E5=85=B3=E9=97=AD=E8=87=AA=E5=8A=A8=E6=8F=90=E4=BA=A4
			con.setAutoCommit(false);
			// =E6=89=A7=E8=A1=8CSQL=E8=AF=AD=E5=8F=A5
			stmt.executeUpdate(sql1);
			stmt.executeUpdate(sql2);
			// =E6=8F=90=E4=BA=A4
			con.commit();
		} catch (SQLException e) {
