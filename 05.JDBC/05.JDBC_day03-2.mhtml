			throw new RuntimeException(e);
		} finally {
			try {
				if (stmt !=3D null) {
					stmt.close();
				}
				if (con !=3D null) {
					con.close();
				}
			} catch (SQLException e) {
				System.out.println("=E9=87=8A=E6=94=BE=E8=B5=84=E6=BA=90=E6=97=B6=E5=8F=
=91=E7=94=9F=E5=BC=82=E5=B8=B8");
			}
		}
	}
