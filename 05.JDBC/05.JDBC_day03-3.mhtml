Code" style=3D"display:none;">package com.tarena.netctoss.impl;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.util.ArrayList;
import java.util.List;

import com.tarena.netctoss.BaseDAO;
import com.tarena.netctoss.RoleInfoDAO;
import com.tarena.netctoss.entity.RoleInfo;

publicclass RoleInfoDAOImpl extends BaseDAO implements RoleInfoDAO {

	privatestaticfinal String FIND_ALL =3D "SELECT role_id,name FROM ROLE_INFO=
";

	privatestaticfinal String MODIFY =3D "UPDATE ROLE_INFO SET name =3D ? WHER=
E role_id=3D?";

	privatestaticfinal String INSERT =3D "INSERT INTO ROLE_INFO(role_id, name)=
 "
			+ " VALUES (ROLE_SEQ.NEXTVAL,?)";

	@Override
	public List&lt;RoleInfo&gt; findAll() throws SQLException {
		Connection conn =3D getConnection();
		String sql =3D FIND_ALL;
		PreparedStatement ps =3D conn.prepareStatement(sql);
		ResultSet rs =3D ps.executeQuery();
		RoleInfo role =3D null;
		List&lt;RoleInfo&gt; list =3D new ArrayList&lt;RoleInfo&gt;();
		while (rs.next()) {
			role =3D new RoleInfo();
			role.setRoleId(rs.getInt("role_id"));
			role.setName(rs.getString("name"));

			list.add(role);
		}
		return list;
	}

	@Override
	public RoleInfo save(RoleInfo role) throws SQLException {
		Connection conn =3D getConnection();
		String sql =3D INSERT;
		PreparedStatement ps =3D conn.prepareStatement(sql,
				new String[] { "role_id" });

		ps.setString(1, role.getName());

		ps.executeUpdate();
		ResultSet rs =3D ps.getGeneratedKeys();
		rs.next();
		int id =3D rs.getInt(1);
		role.setRoleId(id);

		return role;
	}
	@Override
	public RoleInfo modify(RoleInfo role) throws SQLException {
		Connection conn =3D getConnection();
		String sql =3D MODIFY; // =E9=A2=84=E5=85=88=E5=AE=9A=E4=B9=89=E5=A5=BD=
=E7=9A=84SQL=E8=AF=AD=E5=8F=A5

		PreparedStatement ps =3D conn.prepareStatement(sql);
		ps.setString(1, role.getName());
		ps.setInt(2, role.getRoleId());

		int flag =3D ps.executeUpdate();
		return (flag &gt; 0) ? role : null;
	}
}
