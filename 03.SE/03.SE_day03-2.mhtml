public class Cell{
	int row;
	int col;

	public Cell(int row, int col) {
		this.row =3D row;
		this.col =3D col;
	}

	public Cell() {
		this(0, 0);
	}

	public Cell(Cell cell) {
		this(cell.row, cell.col);
	}

	public void drop() {
		row++;
	}

	public void moveRight() {
		col++;
	}

	public void moveLeft() {
		col--;
	}

	@Override
	public String toString() {
		return "(" + row + "," + col + ")";
	}

	@Override
	public boolean equals(Object obj) {
		if (obj =3D=3D null) {
			return false;
		}
		if (this =3D=3D obj) {
			return true;
		}
		if (obj instanceof Cell) {
			Cell cell =3D (Cell) obj;
			return cell.row =3D=3D row &amp;&amp; cell.col =3D=3D col;
		} else {
			return false;
		}
	}
}
