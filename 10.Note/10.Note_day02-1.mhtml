		resultType=3D"com.tarena.entity.Emp"&gt;
		select * from t_emp
		&lt;where&gt;
			&lt;if test=3D"deptno !=3D null"&gt;
				and deptno=3D#{deptno}
			&lt;/if&gt;
			&lt;if test=3D"salary !=3D null"&gt;
