	&lt;select id=3D"findByDept"=20
		parameterType=3D"com.tarena.entity.Condition"
		resultType=3D"com.tarena.entity.Emp"&gt;
		select * from t_emp
		&lt;if test=3D"deptno !=3D null"&gt;
